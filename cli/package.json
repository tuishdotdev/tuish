{
  "name": "@tuish/cli",
  "version": "0.1.0",
  "description": "Tuish developer CLI for signup and product management",
  "type": "module",
  "bin": {
    "tuish": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src/",
    "lint:fix": "biome lint src/ --fix",
    "test": "vitest run",
    "test:integration": "pnpm build && vitest run --config vitest.integration.config.ts",
    "test:watch": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@inkjs/ui": "^2.0.0",
    "chalk": "^5.3.0",
    "conf": "^13.0.1",
    "ink": "^5.0.1",
    "meow": "^13.2.0",
    "open": "^11.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "execa": "^9.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
