{
  "name": "@tuish/sdk",
  "version": "0.1.0",
  "description": "SDK for tuish TUI monetization platform",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./consumer": {
      "types": "./dist/consumer.d.ts",
      "import": "./dist/consumer.js"
    },
    "./admin": {
      "types": "./dist/admin.d.ts",
      "import": "./dist/admin.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@tuish/cli-core": "workspace:*",
    "@tuish/crypto": "workspace:*",
    "@tuish/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "open": "^10.0.0"
  },
  "peerDependenciesMeta": {
    "open": {
      "optional": true
    }
  }
}
