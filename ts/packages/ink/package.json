  },
  "dependencies": {
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@inkjs/ui": "^2.0.0",
    "@tuish/react": "workspace:*",
    "@tuish/sdk": "workspace:*",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.0.0",
    "ink": "^5.0.1",
    "ink-testing-library": "^4.0.0",
    "react": "^18.3.1",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
